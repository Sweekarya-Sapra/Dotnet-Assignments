@model List<Student>

    
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
  @foreach (var item in Model){
  <tr>
      <th scope="row"><a href="https://localhost:44375/Student/Index/@item.Id">@item.Id</a></th>
      <td>@item.FirstName</td>
      <td>@item.LastName</td>
      <td>@item.Email</td>
    </tr>
  }
  </tbody>
</table>
    