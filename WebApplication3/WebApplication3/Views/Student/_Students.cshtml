@model List<Student>

    
   

    <table class="table">
      <thead>

        <tr>
          <th scope="col">#</th>
          <th scope="col">Name</th>
          <th scope="col">Age</th>
          <th scope="col">Phone</th>
          <th scope="col">Edit</th>
          <th scope="col">Delete</th>
        </tr>
      </thead>
      <tbody>
           @foreach (var item in Model)
       {
           <tr>
               
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.Phone</td>
                <td>@Html.ActionLink("Edit","Edit", new { id=@item.Id})</td> 
                <td>@Html.ActionLink("Delete","Delete", new { id=@item.Id})</td>
               
           </tr>
       }
  </tbody>
</table>
 <br />
    <br />
    <center>
    

     <button type="button" class="btn btn-dark"  onclick="location.href='@Url.Action("Insert", "Student")';return false;">Insert a new record</button>
     </center>
     <br />
     <br />

